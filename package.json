{
  "name": "eloquent-ai-chat",
  "version": "0.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "push:consumer": "npm run build && TARBALL=$(npm pack --silent) && CONSUMER=../eloquent.ai-consumer && mv \"$TARBALL\" \"$CONSUMER\" && (cd \"$CONSUMER\" && npm i --no-save --force \"./$TARBALL\") && echo \"Installed $TARBALL in $CONSUMER\"",
    "storybook": "storybook dev -p 6006",
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@storybook/react-vite": "^9.1.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18",
    "storybook": "^9.1.1",
    "tsup": "^8",
    "typescript": "^5"
  }
}
